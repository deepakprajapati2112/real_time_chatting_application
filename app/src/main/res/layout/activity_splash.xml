<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:padding="@dimen/_20sdp"
    tools:context=".splash">

    <ImageView
        android:id="@+id/logoimg"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_300sdp"
        android:layout_marginTop="@dimen/_100sdp"
        android:src="@drawable/ddpchatroom" />
    <TextView
        android:id="@+id/ownone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/logoimg"

        android:layout_marginTop="@dimen/_63sdp"
        android:fontFamily="@font/spartan_thin"
        android:textStyle="bold"
        android:gravity="center"
        android:padding="@dimen/_1sdp"
        android:text="With"
        android:textSize="@dimen/_10sdp"/>

    <TextView
        android:id="@+id/owntwo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ownone"
        android:layout_marginTop="0dp"
        android:fontFamily="@font/apptext"
        android:text="Developer Deepak Prajapati"
        android:textAlignment="center"
        android:textSize="@dimen/_15sdp" />


</RelativeLayout>